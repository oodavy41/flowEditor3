# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.{ts,js}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true


[*.{css,less,scss,sass}]
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
