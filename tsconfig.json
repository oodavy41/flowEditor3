{
  "compilerOptions": {
    "outDir": "./built",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "ES6",
    "allowJs": true,
    "jsx": "react"
  },
  "include": ["./src/**/*"]
}
